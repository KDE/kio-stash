{
    "KDE-KIO-Protocols" : {
        "staging": {
            "Class": ":local",
            "X-DocPath": "kioslave5/kio_staging.html",
            "exec": "kf5/kio/staging",
            "input": "none",
            "listing": [
                "Name",
                "Type",
                "Size",
                "Date",
                "Access",
                "Link"
            ],
            "deleting": true,
            "deleteRecursive": true,
            "fileNameUsedForCopying": "DisplayName",
            "output": "filesystem",
            "protocol": "staging",
            "reading": true,
            "makedir": true,
            "writing": false,
            "renameFromFile": true,
            "renameToFile": true
        }
    }
}
