{
    "KDE-KIO-Protocols" : {
        "stash": {
            "Class": ":local",
            "X-DocPath": "kioslave5/kio_filestash.html",

            "listing": [
                "Name",
                "Type",
                "Size",
                "Date",
                "Access",
                "Link"
            ],

            "exec": "kf5/kio/filestash",
            "input": "none",
            "fileNameUsedForCopying": "Name",
            "output": "filesystem",
            "protocol": "stash",
            "copyFromFile": true,
            "copyToFile": true,
            "renameFromFile": true,
            "renameToFile": true,
            "makedir": true,
            "writing": true,
            "moving": true,
            "reading": true,
            "deleting": true,
            "deleteRecursive": true
        }
    }
}
